# The API key for Google Gemini/Generative AI service
GOOGLE_API_KEY=your_google_api_key_here

# The host to bind to within the container (keep as 0.0.0.0 for Docker)
HOST=0.0.0.0

# The internal port the application listens on (inside the container)
PORT=8000

# The password required to access the web interface
ACCESS_PASSWORD="App-password-here"

# The external port on the host machine to access the application (mapped to internal 8000)
HOST_PORT=8000